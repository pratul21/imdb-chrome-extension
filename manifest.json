{
	"name": "imdb-rating-fetcher",
	"version": "1.0",
	"description": "fetches ratings from imdb",
	"manifest_version": 2,
	"browser_action" : {
		"default_icon": "icon.png",
		"default_popup": "popup.html",
		"default_title": "Click here!"
	},
    "permissions": [
        "activeTab",
        "http://www.omdbapi.com/"
    ]
}